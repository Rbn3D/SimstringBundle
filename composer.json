{
    "name": "mapado/simstring-bundle",
    "type": "symfony-bundle",
    "description": "Mapado Simstring Bundle: Record search",
    "keywords": ["mapado", "simstring", "record search", "symfony"],
    "license": "MIT",
    "autoload": {
        "psr-0": { "Mapado\\SimstringBundle": "src/" },
        "classmap": ["src/Mapado/SimstringBundle/Simstring/simstring.php"]
    },

    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3"
    },
    "require-dev": {
        "raulfraile/ladybug-bundle": "~0.7",
        "atoum/atoum": "dev-master@dev"
    },
    "config": {
        "bin-dir": "bin"
    }
}
